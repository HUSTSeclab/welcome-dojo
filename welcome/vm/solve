echo -n "检查中"
for i in {1..10}
do
	echo -n "."
	sleep 0.1
done
echo -n $'\r'

if grep -q "Welcome to kernel challenge machoke" /run/vm/vm.log && grep -q "machoke initialized" /run/vm/vm.log;
then
	echo "通过！你的 QEMU 虚拟机已经启动！"
	echo -n "你的 flag 是："
	vm exec head /proc/machoke
else
	echo "请确认在 QEMU 虚拟机启动后再获取 flag ！"
fi