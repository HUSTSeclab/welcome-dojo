echo -n "检查中"
for i in {1..10}
do
	echo -n "."
	sleep 0.1
done
echo -n $'\r'

if [ "$TERM_PROGRAM" == "vscode" ]
then
	echo "通过！你确实在VSCode工作空间内运行！"
	echo -n "你的 flag 为： "
	cat /flag
else
	echo "请确认在 VSCode 工作空间运行挑战关卡，而不是通过桌面或 SSH！"
fi
